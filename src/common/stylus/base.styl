body,html
  line-height: 1
  font-weight: 200
  font-family: "PingFang SC", "Helvetica Neue", Helvetica, STHeiTi, Arial, sans-serif

body, div, dl, dt, dd, ul, ol, li, h1, h2, h3, h4, h5, h6, pre, form, fieldset, input, p, blockquote, table, th, td, embed, object
  padding: 0;
  margin: 0;

table
  border-collapse: collapse;
  border-spacing: 0;

fieldset, img, abbr
  border: 0;

address, caption, cite, code, dfn, em,
h1, h2, h3, h4, h5, h6, strong, th, var
  font-weight: normal;
  font-style: normal;

ul
  list-style: none;

caption, th
  text-align: left;

h1, h2, h3, h4, h5, h6
  font-size: 1.0em;

q:before, q:after
  content: "";

a, ins
  color #737e87
  text-decoration: none;

.text-center
  text-align :center

.color-red
  font-weight 700
  color #FF6A6A
